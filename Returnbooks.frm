Private Sub cmblrn_Change()
Dim LRN As String
    Dim FirstName As String, MiddleName As String, LastName As String, Address As String
    
    ' Get the selected LRN
    LRN = cmblrn.Text
    
    ' Find the corresponding record in the Borrowers table (Adodc1)
    memado.Recordset.Find "LRN='" & LRN & "'"
    
    ' Check if the LRN exists
    If Not Adodc1.Recordset.EOF Then
        ' If found, get the details
        FirstName = memado.Recordset("First Name")
        MiddleName = memado.Recordset("Middle Name")
        LastName = memado.Recordset("Last Name")
        
        ' Display details in the textboxes
        txtfn.Text = FirstName
        txtmi.Text = MiddleName
        txtln.Text = LastName
    Else
        ' If not found, clear the textboxes
        txtfn.Text = ""
        txtmi.Text = ""
        txtLastName.Text = ""
        MsgBox "Borrower not found!", vbExclamation
    End If
End Sub

Private Sub Form_Load()
 ' Assuming Adodc1 is the DataControl bound to your Borrowers table
    memado.Recordset.MoveFirst
    cmblrn.Clear ' Clear existing items
    ' Populate the ComboBox with LRN values
    Do While Not memado.Recordset.EOF
        cmblrn.AddItem memado.Recordset("LRN")
        memado.Recordset.MoveNext
    Loop
End Sub

Private Sub txtbookid_Change()
  Dim BookID As String
    BookID = txtbookid.Text

    ' Ensure the user has entered a BookID
    If BookID = "" Then
        Exit Sub
    End If
    
    ' Find the Book Record in the Books Table (Adodc2)
    bookado.Recordset.Find "[Book ID]='" & BookID & "'"
    
    ' Check if the Book Record is found
    If Not bookado.Recordset.EOF Then
        ' If Book is found, display its details in the book-related textboxes
        txttit.Text = bookado.Recordset("Title")
        txtaut.Text = bookado.Recordset("Author")
        txtisbn.Text = bookado.Recordset("ISBN")
        
        ' Check if the book has been borrowed
        If bookado.Recordset("Status") = "Borrowed" Then
            ' If the book is borrowed, display borrower details
            DisplayBorrowerDetails (BookID)
        Else
            ' If the book is available, clear the borrower-related textboxes
            txtfn.Text = ""
            txtln.Text = ""
            txtmi.Text = ""
            txtlrn.Text = ""
            MsgBox "Book is available!", vbInformation
        End If
    Else
        ' If Book is not found, clear the details and show message
        txttit.Text = ""
        txtaut.Text = ""
        txtisbn.Text = ""
        MsgBox "Book not found!", vbExclamation
    End If
End Sub

Private Sub txtlrn_Change()
    txtfn.Text = memado.Recordset("First Name")
    txtmi.Text = memado.Recordset("Middle Name")
    txtln.Text = memado.Recordset("Last Name")
End Sub
